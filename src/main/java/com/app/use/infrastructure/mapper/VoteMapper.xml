<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.app.use.infrastructure.dao.VoteDao">

  <select id="findVote" parameterType="Date" resultType="com.app.use.entity.Vote">
    SELECT
    	*
    FROM
    	vote
    WHERE
    	vote_end_date <![CDATA[ > ]]> #{nowDate}
  </select>

  <select id="findIdVote" parameterType="com.app.use.entity.VoteInfo" resultType="com.app.use.entity.Vote">
    SELECT
    	*
    FROM
    	vote_
    WHERE
    	vote_id = ${vote_id}
  </select>

</mapper>